	"private": true,
	"scripts": {
		"dev": "next -p 3434",
		"build": "next build",
		"start": "next start -p 4343",
		"analyze": "ANALYZE=true yarn build",
		"tsconfig:effective": "tsc --showConfig",
		"postinstall": "npx patch-package",
		"client:codegen": "yarn apollo codegen:generate --localSchemaFile=graphql.schema.json --target=typescript --includes=./lib/graphql/**/*.ts --tagName=gql --addTypename --globalTypesFile=_types/graphql-global-types.ts",
		"codegen:generate": "graphql-codegen -r dotenv/config --config codegen.yml",
		"codegen:combo": "yarn codegen:generate && yarn client:codegen"
	},
	"husky": {
		"hooks": {
			"pre-commit": "yarn tsc && pretty-quick --staged && yarn tsconfig:effective"
		}
	},
