		"husky": "^4.3.0",
		"next-compose-plugins": "^2.2.1",
		"next-unused": "^0.0.3",
		"npm-run-all": "^4.1.5",
		"patch-package": "^6.2.2",
		"postcss": "^8.2.1",
		"postcss-flexbugs-fixes": "^5.0.1",
		"postcss-nesting": "^7.0.1",
		"postcss-preset-env": "^6.7.0",
		"postinstall-postinstall": "^2.1.0",
		"prettier": "^2.2.1",
		"pretty-quick": "^3.1.0",
		"react-ticker": "^1.2.2",
		"stylelint": "^13.8.0",
		"stylelint-config-recommended": "^3.0.0",
		"typescript": "^4.1.3",
		"yaml-loader": "^0.6.0"
	},
	"browserslist": [
		">0.3%",
		"not ie 11",
		"not dead",
		"not op_mini all"
	]
}
