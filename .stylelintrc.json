{
	"extends": "stylelint-config-recommended",
	"rules": {
		"at-rule-no-unknown": [
			true,
			{
				"ignoreAtRules": [
					"extends",
					"tailwind",
					"import",
					"layer",
					"screen",
					"media"
				]
			}
		],
		"block-no-empty": null,
		"unit-whitelist": ["em", "rem", "s", "px", "%", "vw", "vh", "deg"],
		"at-rule-empty-line-before": null
	}
}
